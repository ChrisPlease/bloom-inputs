{
  'presets': ['env', 'react'],
  'plugins': [
    'transform-runtime',
    'transform-object-rest-spread',
    'transform-class-properties',
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./src/components/",
          "styles": "./src/styles/",
          "util": "./src/util/"
        }
      }
    ]
  ]
}
